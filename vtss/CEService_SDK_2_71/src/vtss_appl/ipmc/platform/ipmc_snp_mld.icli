FUNC_REUSE = 

PRIVILEGE = ICLI_PRIVILEGE_15
PROPERTY  = 

CMD_MODE = ICLI_CMD_MODE_GLOBAL_CONFIG
MODE_VAR = 

RUNTIME = 

! 1: ipv6
! 2: mld
! 3: ssm-range
! 4: <ipv6_mcast>
! 5: <ipv6_prefix_length:8-128>

CMD_VAR =
CMD_VAR =
CMD_VAR =
CMD_VAR = v_ipv6_mcast
CMD_VAR = ipv6_prefix_length

HELP = ##ICLI_HELP_IPV6
HELP = ##ICLI_HELP_MLD
HELP = ##HELP_SSM_RANGE
HELP = Valid IPv6 multicast address
HELP = Prefix length ranges from 8 to 128

BYWORD = <HasIpv6 : option>
BYWORD = <HasMld : option>
BYWORD = <HasSsmRange : option>
BYWORD = 
BYWORD = <Ipv6PrefixLength : 8-128>

VARIABLE_BEGIN
    ipmc_prefix_t   icli_ipmcsnp_ssm;
VARIABLE_END

CODE_BEGIN
    memset(&icli_ipmcsnp_ssm, 0x0, sizeof(ipmc_prefix_t));
    IPMC_LIB_ADRS_CPY(&icli_ipmcsnp_ssm.addr.array.prefix, &v_ipv6_mcast);
    icli_ipmcsnp_ssm.len = ipv6_prefix_length;

    if (ipmc_mgmt_set_ssm_range(IPMC_IP_VERSION_MLD, &icli_ipmcsnp_ssm) != VTSS_OK) {
        ICLI_PRINTF("%% Failed to set MLD SSM range.\n\n");
        return ICLI_RC_ERROR;
    }
CODE_END

CMD_END

