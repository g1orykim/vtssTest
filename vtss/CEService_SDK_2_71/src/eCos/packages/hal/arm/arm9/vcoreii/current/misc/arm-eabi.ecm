cdl_option CYGBLD_GLOBAL_COMMAND_PREFIX {
    user_value arm-eabi
};

cdl_option CYGBLD_GLOBAL_CFLAGS {
    user_value "-mcpu=arm9 -Wall -Wpointer-arith -Wstrict-prototypes -Winline -Wundef -Woverloaded-virtual -g -O2 -fno-short-enums -ffunction-sections -fdata-sections -fno-rtti -fno-exceptions -fvtable-gc -finit-priority  -mno-thumb-interwork"
};
