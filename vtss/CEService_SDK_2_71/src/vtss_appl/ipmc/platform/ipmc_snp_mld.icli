CMD_BEGIN

IF_FLAG = 

COMMAND = no ipv6 mld ssm-range

DOC_CMD_DESC    = 
DOC_CMD_DEFAULT = 
DOC_CMD_USAGE   = 
DOC_CMD_EXAMPLE = 

FUNC_NAME = icli_ipmc_snp6_ssm_default
FUNC_REUSE = 

PRIVILEGE = ICLI_PRIVILEGE_15
PROPERTY  = 

CMD_MODE = ICLI_CMD_MODE_GLOBAL_CONFIG
MODE_VAR = 

RUNTIME = 

! 1: no
! 2: ipv6
! 3: mld
! 4: ssm-range

CMD_VAR =
CMD_VAR =
CMD_VAR =
CMD_VAR =

HELP = ##ICLI_HELP_DEFAULT
HELP = ##ICLI_HELP_IPV6
HELP = ##ICLI_HELP_MLD
HELP = ##HELP_SSM_RANGE

BYWORD = <HasDefault : option>
BYWORD = <HasIpv6 : option>
BYWORD = <HasMld : option>
BYWORD = <HasSsmRange : option>

VARIABLE_BEGIN
    ipmc_prefix_t   icli_ipmcsnp_ssm;
VARIABLE_END

CODE_BEGIN
    memset(&icli_ipmcsnp_ssm, 0x0, sizeof(ipmc_prefix_t));
    icli_ipmcsnp_ssm.addr.array.prefix.addr[0] = (IPMC_DEF_SSM_PREFIX6_VALUE >> 24) & 0xFF;
    icli_ipmcsnp_ssm.addr.array.prefix.addr[1] = (IPMC_DEF_SSM_PREFIX6_VALUE >> 16) & 0xFF;
    icli_ipmcsnp_ssm.addr.array.prefix.addr[2] = (IPMC_DEF_SSM_PREFIX6_VALUE >> 8) & 0xFF;
    icli_ipmcsnp_ssm.addr.array.prefix.addr[3] = (IPMC_DEF_SSM_PREFIX6_VALUE >> 0) & 0xFF;
    icli_ipmcsnp_ssm.len = IPMC_DEF_SSM_PREFIX6_LEN_VALUE;

    if (ipmc_mgmt_set_ssm_range(IPMC_IP_VERSION_MLD, &icli_ipmcsnp_ssm) != VTSS_OK) {
        ICLI_PRINTF("%% Failed to default MLD SSM range.\n\n");
        return ICLI_RC_ERROR;
    }
CODE_END

CMD_END

